{
  "apiVersion": "extensions/v1beta1",
  "kind": "Ingress",
  "metadata": {
    "name": "atlas-api-ingress",
    "annotations": {
      "external-dns.alpha.kubernetes.io/hostname": "api-dev.mykro.be"
    },
    "namespace": "default"
  },
  "spec": {
    "backend": {
      "serviceName": "atlas-api-service",
      "servicePort": 3000
    },
    "rules": [
      {
        "host": "api-dev.mykro.be",
        "http": {
          "paths": [
            {
              "backend": {
                "serviceName": "atlas-api-service",
                "servicePort": 3000
              }
            }
          ]
        }
      }
    ]
  }
}
