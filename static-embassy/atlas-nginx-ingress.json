{
  "apiVersion": "extensions/v1beta1",
  "kind": "Ingress",
  "metadata": {
    "name": "atlas-ingress",
    "annotations": {
      "nginx.ingress.kubernetes.io/rewrite-target": "/"
    }
  },
  "spec": {
    "rules": [
      {
        "host": "dev-mykrobe-atlas.makeandship.com",
        "http": {
          "paths": [
            {
              "path": "/*",
              "backend": {
                "serviceName": "atlas-service",
                "servicePort": 3000
              }
            }
          ]
        }
      },
      {
        "host": "api-dev-mykrobe-atlas.makeandship.com",
        "http": {
          "paths": [
            {
              "path": "/*",
              "backend": {
                "serviceName": "atlas-api-service",
                "servicePort": 3000
              }
            }
          ]
        }
      }
    ] 
  }
}


