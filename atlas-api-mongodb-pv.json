{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "atlas-api-mongodb-pv",
    "namespace": "mykrobe"
  },
  "spec": {
    "storageClassName": "manual",
    "capacity": {
      "storage": "8Gi"
    },
    "accessModes": ["ReadWriteOnce"],
    "hostPath": {
      "path": "/home/ec2-user/gocd/data"
    }
  }
}
