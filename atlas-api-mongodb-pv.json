{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "atlas-api-mongodb-pv"
  },
  "spec": {
    "storageClassName": "manual"
  },
  "capacity": {
    "storage": "8Gi"
  },
  "accessModes": [
    "ReadWriteOnce"
  ],
  "hostPath": {
    "path": "/home/ec2-user/gocd/data"
  }
}