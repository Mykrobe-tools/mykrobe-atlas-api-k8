{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "postgres",
        "namespace": "default",
        "labels": {
          "app": "postgres"
        }
    },
    "spec": {
        "type": "NodePort",
        "ports": [
          {
            "protocol": "TCP",
            "targetPort": 5432
          }
        ],
        "selector": {
          "app": "postgres"
        }
    }
}

  