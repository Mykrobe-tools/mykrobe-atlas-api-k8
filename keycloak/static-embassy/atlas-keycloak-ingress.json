{
  "apiVersion": "extensions/v1beta1",
  "kind": "Ingress",
  "metadata": {
    "name": "atlas-keycloak-ingress",
    "annotations": {
      "kubernetes.io/ingress.class": "nginx",
      "cert-manager.io/cluster-issuer": "letsencrypt-prod"
    }
  },
  "spec": {
    "backend": {
      "serviceName": "keycloak-service",
      "servicePort": 8080
    },
    "tls": [
      {
        "hosts": [
          "keycloak-dev-static.mykro.be"
        ],
        "secretName": "keycloak-dev-static-mykro-be-tls"
      }
    ],
    "rules": [
      {
        "host": "keycloak-dev-static.mykro.be",
        "http": {
          "paths": [
            {
              "backend": {
                "serviceName": "keycloak-service",
                "servicePort": 8080
              }
            }
          ]
        }
      }
    ]
  }
}