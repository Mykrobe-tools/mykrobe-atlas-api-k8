{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "postgres-pv-volume",
    "namespace": "default",
    "labels": {
      "type": "local",
      "app": "postgres"
    }
  },
  "spec": {
    "capacity": {
      "storage": "10Gi"
    },
    "accessModes": ["ReadWriteMany"],
    "hostPath": {
      "path": "/home/atlas/keycloak/data"
    }
  }
}
