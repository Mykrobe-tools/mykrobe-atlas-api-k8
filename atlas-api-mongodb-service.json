{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "atlas-api-mongodb",
    "labels": {
      "name": "atlas-api-mongodb"
    },
    "namespace": "mykrobe"
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "port": 27017,
        "targetPort": 27017
      }
    ],
    "selector": {
      "app": "atlas-api-mongodb"
    }
  }
}
